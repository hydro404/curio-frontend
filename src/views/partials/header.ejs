<% let grandTotal = 0; cartItems.forEach(function(item) {
  grandTotal += item.quantity * item.price; }); %>
<!-- Remove "navbar-sticky" class to make navigation bar scrollable with the page.-->
<div class="navbar-sticky bg-light">
  <div class="navbar navbar-expand-lg navbar-light">
    <div class="container">
      <a class="navbar-brand d-none d-sm-block flex-shrink-0" href="/"
        ><img src="img/logo.svg" width="170" alt="Curio 4552" /></a
      ><a class="navbar-brand d-sm-none flex-shrink-0 me-2" href="/"
        ><img src="img/logo.svg" width="100" alt="Curio 4552"
      /></a>

      <div class="navbar-toolbar d-flex flex-shrink-0 align-items-center">
        <a
          class="navbar-tool ms-1 ms-lg-0 me-n1 me-lg-2"
          href="<%= loggedIn ? '/account-profile-info' : '#signin-modal' %>"
          <% if (!loggedIn) { %>data-bs-toggle="modal"<% } %>
        >
          <div class="navbar-tool-icon-box">
            <i class="navbar-tool-icon ci-user"></i>
          </div>
          <div class="navbar-tool-text ms-n3">
            <small>Hello, <%= (loggedIn && userDetails.firstname) ? userDetails.firstname : (loggedIn ? 'User' : 'Sign In') %></small>My Account
          </div>
        </a>
        <div class="navbar-tool dropdown ms-3">
          <a
            class="navbar-tool-icon-box bg-secondary dropdown-toggle"
            href="<%= loggedIn ? '/cart' : '#signin-modal' %>" <% if (!loggedIn) { %>data-bs-toggle="modal"<% } %>
            ><span class="navbar-tool-label"><%- totalCartItems %></span
            ><i class="navbar-tool-icon ci-cart"></i></a
          ><a class="navbar-tool-text" href="<%= loggedIn ? '/cart' : '#signin-modal' %>" <% if (!loggedIn) { %>data-bs-toggle="modal"<% } %>>
            <small>My Cart</small>&#8369; <%= grandTotal.toFixed(2) %></a
          >
          <!-- Cart dropdown-->
          <div class="dropdown-menu dropdown-menu-end" id="dropdownCart" style="width: 300px !important;">
            <div class="widget widget-cart px-3 pt-2 pb-3" id="myWidget">
              <div style="height: 15rem;" data-simplebar="init" data-simplebar-auto-hide="false">
                  <div class="simplebar-wrapper" style="margin: 0px -16px 0px 0px;">
                      <div class="simplebar-height-auto-observer-wrapper">
                          <div class="simplebar-height-auto-observer"></div>
                      </div>
                      <div class="simplebar-mask">
                          <div class="simplebar-offset" style="right: 0px; bottom: 0px;">
                              <div class="simplebar-content-wrapper" tabindex="0" role="region" aria-label="scrollable content" style="height: 100%; overflow: hidden scroll;">
                                  <div class="simplebar-content" style="padding: 0px 16px 0px 0px;" id="cartContainer">
                
          
                                          <% if(cartItems && cartItems.length!==0){ %>
                                              <% cartItems.forEach(function(item) { %>
                                                      <div class="widget-cart-item py-2 border-bottom">
                                                          <button class="btn-close text-danger" type="button" aria-label="Remove" onclick="removefromCart('<%= item.product_id %>')"><span aria-hidden="true">×</span></button>
                                                          <div class="d-flex align-items-center"><a class="flex-shrink-0" href="/product?id=<%= item.product_id %>">
                                                            <% var images = item.images[0]; %>

                                                            <img src="img/<%= images.path %>" width="64" alt="Product"/></a>
                                        
                                                          </a>
                                                          <div class="ps-2">
                                                              <h6 class="widget-product-title"><a href="/product?id=<%= item.product_id %>"><%= item.name %></a></h6>
                                                              <div class="widget-product-meta"><span class="text-primary me-2 textmask">₱&nbsp;<%= item.price %></span><span class="text-muted">x <%= item.quantity %></span></div>
                                                          </div>
                                                          </div>
                                                      </div>
                                              <% }) %>
                                          <% }else{ %>
                                              <div class="container text-center py-2 my-2 my-md-3">
                                                  <img src="data:image/png;base64," width="60" class="d-block mx-auto mb-4" alt="Empty cart">
                                                  <p class="cart-empty woocommerce-info">Your cart is currently empty.</p>
                                                  <div class="return-to-shop">
                                                      <a class="button wc-backward btn btn-primary" href="/products">
                                                          <i class="czi-arrow-left mr-1 ml-n1"></i>
                                                          Return to shop			
                                                      </a>
                                                  </div>
                                              </div>
                                          <% } %>
      
      
      
                                  </div>
                              </div>
                          </div>
                      </div>
                      <div class="simplebar-placeholder" style="width: auto; height: 316px;"></div>
                  </div>
                  <div class="simplebar-track simplebar-horizontal" style="visibility: hidden;">
                      <div class="simplebar-scrollbar simplebar-visible" style="width: 0px; display: none;"></div>
                  </div>
                  <div id="simplebarID">
                      <% if (cartItems && cartItems.length > 3) { %>
                          <div class="simplebar-track simplebar-vertical" style="visibility: visible;">
                            <div class="simplebar-scrollbar simplebar-visible" style="height: 182px; display: block; transform: translate3d(0px, 0px, 0px);"></div>
                          </div>
                      <% } %>
                  </div>
                  
              </div>
              
              <div id="footerCart">
                      <div class="d-flex flex-wrap justify-content-between align-items-center py-3">
              
                        <div class="fs-sm me-2 py-2"><span class="text-muted">Subtotal:</span>
                          <span class="text-primary fs-base ms-1 textmask" id="overAllz">
                              ₱&nbsp;<%= grandTotal.toLocaleString('en-US') %>
                          </span>
      
                      </div>
                      
                      <a class="btn btn-outline-secondary btn-sm" href="/cart">Expand cart<i class="ci-arrow-right ms-1 me-n1"></i></a>
                      </div>
                      <% if(cartItems && cartItems.length!==0){ %>
                          <a class="btn btn-primary btn-sm d-block w-100" href="/checkout"><i class="ci-card me-2 fs-base align-middle"></i>Checkout</a>
                      <% } %>
              </div>
              
            </div>
          </div>


        </div>
      </div>
    </div>
  </div>
</div>
